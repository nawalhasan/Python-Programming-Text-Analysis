    "labels = pd.read_csv('https://raw.githubusercontent.com/ariedamuco/Python-Programming-and-Text-Analysis/master/Inputs/spam-reduced/labels.csv')\n",
    "output_df = final_df.merge(labels, how='inner', on='id')"
