    "# creating a for loop that takes all .txt files, opens the file, removes punctuations\n",
    " \n",
    "for file in glob.glob('*.txt'): \n",
    "    for line in open(file): \n",
    "        print(remove_punctuation(line), end='') "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "cd1d1433",
   "metadata": {},
   "outputs": [],
   "source": [
    "# writing out the edited files \n",
    "# eg : # a.txt => a-out.txt\n",
    "\n",
    "for file in glob.glob('*.txt'): \n",
    "    with open(file.replace('.txt', '-out.txt'), 'w') as outfile: \n",
    "        with open(file) as infile: \n",
    "            for line in infile: \n",
    "                print(remove_punctuation(line), end='', file=outfile) \n",
    "                "
   ]
  },
